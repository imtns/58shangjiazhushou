.mod-information-logo{
    width: 128rpx;
    height: 128rpx;
    border: solid 4rpx #F2B839;
    border-radius: 100%;
}

.mod-information-logo-58{
    position: relative;
    z-index: 10;
}

.mod-information-logo-58-text{
    font-size: 20rpx;
    background-color: #F2B839;
    color: #fff;
    padding: 6rpx 18rpx;
    border-radius: 16rpx;
    font-weight: bold;
}

.mod-information-name{
    font-size: 37rpx;
    line-height: 52rpx;
    font-weight: bold;
    margin: 26rpx 0 15rpx;
}

.mod-information-score{
