            this.business = Object.assign({}, this.business, {
                changeMP: false,
                canConfirm: false,
            });
            this.$apply();
        },
        selectMp (item) {
            this.business = Object.assign({}, this.business, {
                chosedId: item.id,
                canConfirm: true,
            });
            console.log('点击', this.business);
            this.$apply();
        },
        confirmAction () {
            if (this.business.selected.id) {
                this.setGlobalMpId(this.business.chosedId);
                this.loadData();
                this.hideMask();
            }
        },
