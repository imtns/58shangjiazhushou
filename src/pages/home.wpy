            {
                title: '店铺运营',
                items: [
                    {
                        iconPath: 'https://static.58.com/lbg/shangjiaxcxht/zhushou/img/icon-store-fit.png',
                        text: '店铺装修',
                    }, {
                        iconPath: 'https://static.58.com/lbg/shangjiaxcxht/zhushou/img/icon-order-service.png',
