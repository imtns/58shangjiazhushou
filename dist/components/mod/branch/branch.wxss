.mod-branch{
    height: 88rpx;
    padding:0 30rpx 0;
    position:static;
}
.mod-branch-none{
    display: none;
}
.branch-main{
    display: flex;
}
.branch-main-title,.branch-main-text{
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    padding-top:24rpx;
}
.branch-main-title{
    width:112rpx;
    color: #333333;
}
.branch-main-text{
    flex:1;
    margin-left:36rpx;
    padding-right:36rpx;
    text-align: right;
    color: #999;
}
.branch-main-text::after{
    display: inline-block;
    content:'';
    width:16rpx;
    height:26rpx;
    background:url('data:image/png;base64,') no-repeat top left;
    background-size:100%;
    position:relative;
    left:20rpx;
    top:6rpx;
}
.branch-chooice-mask{
    position:absolute;
    top:0;
    bottom:0;
    left:0;
    right:0;
    z-index: 99;
    background:rgba(0,0,0,.6);
}
.branch-chooice-container{
    width:100%;
    position:fixed;
    bottom:0;
    left:0;
    z-index: 100;
    background:#fff;
}
.branch-chooice-title{
    line-height: 110rpx;
    text-align: center;
    font-size: 36rpx;
    color: #666;
    position:relative;
    overflow: hidden;
}
.branch-chooice-title::after{
    display: block;
    content: "\e69a";
    color:#999;
    font-size:36rpx;
    position:absolute;
    right:30rpx;
    top:0;
}
.branch-chooice-list{
    height: 508rpx;
    padding:0 30rpx;
    border-top:2rpx solid #f3f3f3;
}
.branch-chooice-list-item{
    padding:32rpx 0 26rpx;
    overflow: hidden;
    border-bottom:2rpx solid #f3f3f3;
}
.last-branch-chooice-list-item{
    border-bottom:none;
}
.branch-chooice-list-item-name{
    font-size: 28rpx;
    color: #333;
    line-height: 28rpx;
}
.branch-chooice-list-item-address,.branch-chooice-list-item-call{
    margin-top:10rpx;
}
.branch-chooice-list-item-address{
    float:left;
    margin-right:100rpx;
    width:518rpx;
    font-size: 26rpx;
    color: #999;
    overflow: hidden;
    display:-webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2; 
    text-overflow: ellipsis; 
}
.branch-chooice-list-item-call{
    overflow: hidden;
    width:40rpx;
    height: 40rpx;
    vertical-align: top;
    background: url("data:image/png;base64,")
    no-repeat top left;
    background-size: 100% 100%;
}