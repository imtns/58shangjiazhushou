@charset "UTF-8";
view {
  box-sizing: border-box;
}

.coupons .coupon-list-item {
  width: 710rpx;
  height: 180rpx;
  margin: 30rpx 20rpx 0 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-shadow: 0 1rpx 10rpx 0 rgba(0, 0, 0, 0.05);
  border-radius: 4rpx;
  position: relative;
  -webkit-transition: margin .5s;
  transition: margin .5s;
}

.coupons .coupon-list-item.editing {
  margin-left: 123rpx;
}

.coupons .coupon-list-item .coupon-list-left {
  width: 176rpx;
  color: #FF552E;
  font-size: 30rpx;
  background-color: #fff;
  line-height: 180rpx;
  padding: 0 20rpx;
  border-right: 2rpx dashed #FFB204;
  border-radius: 4rpx 0 0 4rpx;
  text-align: center;
}

.coupons .coupon-list-item .coupon-list-left .discount .bold {
  font-size: 70rpx;
}

.coupons .coupon-list-item .coupon-list-left .reduce .bold {
  font-size: 60rpx;
}

.coupons .coupon-list-item .coupon-list-middle {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  background-image: linear-gradient(135deg, #FFBD05 0%, #FF9602 100%);
  padding: 30rpx 24rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}

.coupons .coupon-list-item .coupon-list-middle .info {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
  color: #FFF;
}

.coupons .coupon-list-item .coupon-list-middle .info .title {
  font-size: 32rpx;
  line-height: 45rpx;
}

.coupons .coupon-list-item .coupon-list-middle .info .type {
  font-size: 28rpx;
  line-height: 40rpx;
  margin-bottom: 13rpx;
}

.coupons .coupon-list-item .coupon-list-middle .info .time {
  font-size: 24rpx;
  color: #ffe37f;
  line-height: 33rpx;
}

