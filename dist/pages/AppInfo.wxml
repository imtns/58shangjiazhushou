
    <view class="appinfo-container" hidden="{{loading}}">
        <view class="appinfo-base">
            <image class="appinfo-logo" src="{{logo}}"/>
            <view class="appinfo-title-wrap">
                <view class="appinfo-title">{{appInfo.nickName}}</view>
                <view class="appinfo-date">有效期至 {{appInfo.date}}</view>
            </view>
            <view class="appinfo-youxiang {{(appInfo.app_type==1 || appInfo.app_type==2)?'':'disable'}}">优享</view>
            <view class="appinfo-weixin {{appInfo.app_type==2?'':'disable'}}">微信</view>
        </view>
        <view class="appinfo-status">
            <view class="appinfo-auth"  wx:if="{{appInfo.app_type==2}}">授权状态：{{appInfo.status_desc}}</view>
            <view class="appinfo-verify">审核状态：{{appInfo.lastpublishState_desc}}</view>
            <view class="appinfo-telephone">联系电话：{{appInfo.telphone}}</view>
            <view class="appinfo-address">小程序地址：{{appInfo.address}}</view>
        </view>
        <view class="appinfo-codelist">
            <!-- 优享小程序码 -->
            <view class="appinfo-codelist-li"  wx:if="{{appInfo.wbQrcodeUrl}}">
                <image class="qrcode-image" src="{{wbQrcodeUrl}}" mode="aspectFill" bindtap="bindClickPreview" data-wpybindclickpreview-a="{{appInfo.wbQrcodeUrl}}"/>
                <view class="qrcode-tip">点击保存</view>
                <view class="qrcode-name">优享小程序码</view>
                <!-- 访问优享小程序 -->
                <navigator target="miniProgram" open-type="navigate" app-id="wx86c7b0019914401c" path="/pages/index58/index58?releaseId={{appInfo.mpid}}" url="/pages/home" version="develop">
                    <view class="qrcode-visit">访问小程序</view>
                </navigator>
            </view>
            <view class="appinfo-codelist-li" wx:else>
                <view class="qrcode-image-empty">上线后展示</view>
                <view class="qrcode-name">优享小程序码</view>
            </view>
            <!-- 微信小程序码 -->
            <view class="appinfo-codelist-li" wx:if="{{appInfo.qrcodeUrl}}">
                <image class="qrcode-image" src="{{qrcodeUrl}}" mode="aspectFill" bindtap="bindClickPreview" data-wpybindclickpreview-a="{{appInfo.qrcodeUrl}}"/>
                <view class="qrcode-tip">点击保存</view>
                <view class="qrcode-name">微信小程序码</view>
                <!-- 访问微信小程序 -->
                <view class="qrcode-name">点击长按识别</view>
            </view>
            <!-- 小程序支付码 -->
            <view class="appinfo-codelist-li last" wx:if="{{appInfo.payQrcodeUrl}}">
                <image class="qrcode-image" src="{{payQrcodeUrl}}" mode="aspectFill" bindtap="bindClickPreview" data-wpybindclickpreview-a="{{appInfo.payQrcodeUrl}}"/>
                <view class="qrcode-tip">点击保存</view>
                <view class="qrcode-name">小程序支付码</view>
            </view>
            <view class="appinfo-codelist-li last" wx:else>
                <view class="qrcode-image-empty">
                    <view>需开通支付</view>
                    <view>功能后显示</view>
                </view>
                <view class="qrcode-name">小程序支付码</view>
                <!-- 跳转开通支付页面 -->
                <navigator open-type="navigate" url="/pages/OpenPay">
                    <view class="qrcode-openpay">去开通</view>
                </navigator>
            </view>
        </view>
        <navigator url="/pages/AppEdit?mpid={{appInfo.mpid}}"><view class="appinfo-edit">编辑小程序</view></navigator>
    </view>