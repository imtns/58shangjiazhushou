
    <view class="appedit-container" hidden="{{loading}}">
        <view class="appedit-header" bindtap="setLogo"  wx:if="{{form.app_type==1 || !form.headImg}}">
            <text class="appedit-form-label">上传小程序标志</text>
            <image class="appedit-form-logo" mode="aspectFill" src="{{logoUrl}}"/>
            <text class="appedit-form-arrow"/>
        </view>
        <view class="appedit-header"  wx:else>
            <text class="appedit-form-label">上传小程序标志</text>
            <image class="appedit-form-logo" mode="aspectFill" src="{{logoUrl}}"/>
        </view>
        <view class="appedit-content-list">
            <view class="appedit-content-list-item">
                <text class="appedit-form-label">小程序名称</text>
                <input class="appedit-form-input" type="text" bindinput="setAppName" placeholder="提交后优享小程序才可修改" placeholder-class="appedit-form-placeholder" maxlength="30" confirm-type="done" value="{{form.nickName}}" wx:if="{{form.app_type==1}}"/>
                <view class="appedit-form-appname appedit-form-placeholder" type="text" wx:else>{{form.nickName}}</view>
            </view>
            <view class="appedit-content-list-item">
                <text class="appedit-form-label">联系电话</text>
                <input class="appedit-form-input" maxlength="11" type="number" bindinput="setMobile" placeholder="请输入" placeholder-class="appedit-form-placeholder" value="{{form.telphone}}"/>
            </view>
            <!-- 同镇用户所属行业只可编辑一次，mpSource:3 代表同镇，所属行业已编辑 -->
            <view class="appedit-content-list-item" wx:if="{{!cateEditable}}">
                <text class="appedit-form-label">所属行业</text>
                <view class="appedit-form-cate">
                    {{ form.cate1Name+' '+form.cate2Name+' '+form.cate3Name }}
                </view>
                <text class="appedit-form-arrow"/>
            </view>
            <!-- <navigator url="/pages/CateSelector?cate1={{form.cate1}}&cate2={{form.cate2}}&cate3={{form.cate3}}" wx:else> -->
            <navigator url="/pages/multiSelector?type=trade" wx:else>
                <view class="appedit-content-list-item">
                    <text class="appedit-form-label">所属行业</text>
                    <view class="appedit-form-cate">
                        <block wx:if="{{form.cate1Name}}">
                            {{ form.cate1Name+' '+form.cate2Name+' '+form.cate3Name }}
                        </block>
                        <block wx:else>
                            请选择
                        </block>
                    </view>
                    <text class="appedit-form-arrow"/>
                </view>
             </navigator>
            <picker mode="multiSelector" bindchange="setCity" bindcolumnchange="bindCity" range="{{areas}}" range-key="localName" value="{{selCityIndex}}">
                <view class="appedit-content-list-item">
                    <text class="appedit-form-label">小程序城市</text>
                    <view class="appedit-form-picker" wx:if="{{form.city}}">
                        {{form.provinceName}} {{form.cityName}}
                    </view>
                    <view class="appedit-form-picker-placeholder" wx:else>
                        请选择
                    </view>
                </view>
            </picker>
            <view class="appedit-content-list-item appedit-content-list-item-noborder">
                <text class="appedit-form-label-address">小程序具体地点</text>
                <view class="appedit-form-btn-location" bindtap="setLocation">定位</view>
            </view>
            <view class="appedit-content-list-item appedit-content-list-item-noborder">
                <textarea name="" id="" cols="30" rows="10" class="appedit-form-input-addressdetail" placeholder="请输入" placeholder-class="appedit-form-placeholder" bindinput="setDetailAdress" value="{{form.address}}"/>
            </view>
        </view>
        <view class="appedit-map">
            <map id="map" class="appedit-map-in" longitude="{{longitude}}" latitude="{{latitude}}" scale="14" markers="{{markers}}"/>
        </view>
        <view class="appedit-submit submit-enable" bindtap="bindSave" wx:if="{{enableSave}}">保存</view>
        <view class="appedit-submit" bindtap="bindSave" wx:else>保存</view>
        
    <view class="wx-content-info"  wx:if="{{ $cropper$imageSrc }}">
        <view wx:if="{{ $cropper$isShowImg }}" class="wx-corpper" style="width:{{$cropper$cropperInitW}}rpx;height:{{$cropper$cropperInitH}}rpx;">
            <view class="wx-corpper-content" style="width:{{$cropper$cropperW}}rpx;height:{{$cropper$cropperH}}rpx;left:{{$cropper$cropperL}}rpx;top:{{$cropper$cropperT}}rpx">
                <image src="{{ $cropper$imageSrc }}" style="width:{{$cropper$cropperW}}rpx;height:{{$cropper$cropperH}}rpx"/>
                <view class="wx-corpper-crop-box" bindtouchstart="$cropper$contentStartMove" bindtouchmove="$cropper$contentMoveing" style="width:{{$cropper$cutW}}rpx;height:{{$cropper$cutH}}rpx;left:{{$cropper$cutL}}rpx;top:{{$cropper$cutT}}rpx">
                    <view class="wx-cropper-view-box">
                        <block wx:if="{{ !$cropper$ratio }}">
                            <view class="wx-cropper-dashed-h"/>
                            <view class="wx-cropper-dashed-v"/>
                        </block>
                        <view class="wx-cropper-line-t" data-drag="top" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                        <view class="wx-cropper-line-r" data-drag="right" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                        <view class="wx-cropper-line-b" data-drag="bottom" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                        <view class="wx-cropper-line-l" data-drag="left" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                        <block wx:if="{{ !$cropper$ratio }}">
                            <view class="wx-cropper-point point-t" data-drag="top" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                            <view class="wx-cropper-corner-point point-tr" data-drag="topTight"/>
                            <view class="wx-cropper-point point-r" data-drag="right" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                            <view class="wx-cropper-point point-b" data-drag="bottom" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                            <view class="wx-cropper-corner-point point-bl" data-drag="bottomLeft"/>
                            <view class="wx-cropper-point point-l" data-drag="left" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                            <view class="wx-cropper-corner-point point-lt" data-drag="leftTop"/>
                        </block>
                        <view class="wx-cropper-corner-point point-rb" data-drag="rightBottom" catchtouchstart="$cropper$dragStart" catchtouchmove="$cropper$dragMove"/>
                    </view>
                </view>
                <view class="crop-mask" style="border-top-width:{{$cropper$cutT}}rpx;border-right-width:{{$cropper$cropperW-$cropper$cutL-$cropper$cutW}}rpx;border-bottom-width:{{$cropper$cropperH-$cropper$cutT-$cropper$cutH}}rpx;border-left-width:{{$cropper$cutL}}rpx;"/>
            </view>
        </view>
        <canvas canvas-id="myCanvas" style="position:absolute;border: 1px solid red; width:{{$cropper$qualityWidth}}px;height:{{$cropper$qualityWidth / $cropper$innerAspectRadio}}px;top:600rpx;left:-9999px"/>
        <canvas canvas-id="myCanvas2" style="position:absolute;border: 1px solid red; width:{{$cropper$canvasWidth}}px;height:{{$cropper$canvasWidth}}px;top:600rpx;right:-9999px;"/>
        <view class="options">
            <view class="option-item restore" bindtap="$cropper$restore">还原</view>
            <view class="option-item rotate" bindtap="$cropper$rotateImage">旋转</view>
            <view class="option-item cancel" bindtap="$cropper$cancel">取消</view>
            <view class="option-item crop" bindtap="$cropper$getImageInfo">裁剪</view>
        </view>
  </view>

    </view>