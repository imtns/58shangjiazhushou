.appedit-container {
  box-sizing: border-box;
  background: #f6f6f6;
  width: 100vw;
  height: 100vh;
  padding: 20rpx 0 30rpx;
}

.appedit-container .appedit-header {
  height: 110rpx;
  margin-bottom: 38rpx;
  background: #fff;
  padding: 0 30rpx;
}

.appedit-container .appedit-header::after {
  display: inline-block;
  content: '';
  width: 0;
  height: 110rpx;
  line-height: 110rpx;
  vertical-align: middle;
}

.appedit-container .appedit-header .appedit-form-label, .appedit-container .appedit-header .appedit-content-list .appedit-content-list-item .appedit-form-label-address, .appedit-container .appedit-content-list .appedit-content-list-item .appedit-header .appedit-form-label-address {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #999999;
  vertical-align: middle;
}

.appedit-container .appedit-header .appedit-form-logo {
  margin-left: 364rpx;
  width: 72rpx;
  height: 72rpx;
  vertical-align: middle;
  border-radius: 50%;
}

.appedit-container .appedit-header .appedit-form-arrow {
  display: inline-block;
  content: '';
  width: 15rpx;
  height: 25rpx;
  vertical-align: middle;
  margin-left: 25rpx;
  background: url("https://static.58.com/lbg/mengchong/image/icon/arrow-right.png") no-repeat;
  background-size: 100%;
}

.appedit-container .appedit-content-list {
  padding: 0 30rpx 40rpx;
  background: #fff;
}

.appedit-container .appedit-content-list .appedit-content-list-item {
  height: 90rpx;
  border-bottom: 2rpx solid #f3f3f3;
}

.appedit-container .appedit-content-list .appedit-content-list-item::after {
  display: inline-block;
  content: '';
  width: 0;
  height: 90rpx;
  line-height: 90rpx;
  vertical-align: middle;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-label, .appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-label-address {
  display: inline-block;
  width: 150rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #999999;
  vertical-align: middle;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-placeholder {
  color: #ccc;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-input,
.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-picker,
.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-appname,
.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-cate,
.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-picker-placeholder {
  margin-left: 144rpx;
  display: inline-block;
  width: 396rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
  text-align: right;
  vertical-align: middle;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-cate {
  margin-left: 144rpx;
  display: inline-block;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
  text-align: right;
  vertical-align: middle;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-appname {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-picker-placeholder {
  color: #ccc;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-label-address {
  width: 210rpx;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-btn-location {
  display: inline-block;
  margin-left: 392rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #4e88ff;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-btn-location::before {
  display: inline-block;
  content: '';
  width: 24rpx;
  height: 30rpx;
  background: url("https://img.58cdn.com.cn/lbg/shangjiaxcxht/zhushou/img/icon_location.png") no-repeat top left;
  background-size: 100%;
  margin-right: 4rpx;
  vertical-align: -4rpx;
}

.appedit-container .appedit-content-list .appedit-content-list-item .appedit-form-input-addressdetail {
  display: block;
  width: 100%;
  height: 88rpx;
  padding-left: 0;
  text-indent: 0;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
}

.appedit-container .appedit-content-list .appedit-content-list-item-noborder {
  border: none;
}

.appedit-container .appedit-map {
  display: block;
  height: 360rpx;
}

.appedit-container .appedit-map .appedit-map-in {
  display: block;
  width: 100%;
  height: 360rpx;
}

.appedit-container .appedit-submit {
  width: 690rpx;
  line-height: 94rpx;
  margin: 60rpx auto 0;
  background: #cccccc;
  border-radius: 4rpx;
  font-family: PingFangSC-Regular;
  font-size: 3r2px;
  color: #ffffff;
  text-align: center;
}

.appedit-container .submit-enable {
  background: #4e88ff;
}
