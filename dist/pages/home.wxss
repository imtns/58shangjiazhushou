view,
scroll-view {
  box-sizing: border-box;
}

.home {
  width: 100%;
  height: 1206rpx;
  background: #f6f6f6;
}

.home .mask {
  width: 100%;
  height: 100%;
  position: absolute;
  background: rgba(0, 0, 0, 0.5);
  top: 0;
}

.home .mpBox {
  width: 540rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
          transform: translate3d(-50%, -50%, 0);
  background: white;
  border-radius: 2rpx;
}

.home .mpBox .name {
  width: 400rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.home .mpBox .haslogo {
  width: 54rpx;
  height: 54rpx;
  position: absolute;
  left: 20rpx;
  top: 30rpx;
  border-radius: 100rpx;
}

.home .mpBox .logoPadding {
  padding-left: 45rpx;
}

.home .mpBox .default {
  padding: 0 40rpx;
}

.home .mpBox .default {
  position: relative;
  height: 113rpx;
  font-size: 28rpx;
  color: #bdbdbd;
  line-height: 113rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}

.home .mpBox .otherBox {
  font-size: 32rpx;
  color: #666666;
  overflow: scroll;
  max-height: 320rpx;
}

.home .mpBox .otherBox .mpbox {
  position: relative;
  padding: 0 40rpx;
}

.home .mpBox .otherBox .mpItem {
  height: 113rpx;
  line-height: 113rpx;
  border-top: 1rpx solid #f6f6f6;
  padding-right: 80rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.home .mpBox .otherBox .mpItem .logoPadding {
  padding-left: 45rpx;
}

.home .mpBox .otherBox .active {
  background: url(https://static.58.com/lbg/shangjiaxcxht/zhushou/img/correct-home.png) no-repeat right;
  background-size: 34.5rpx 34.5rpx;
  color: #4e88ff;
}

.home .mpBox .btnBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 113rpx;
  width: 100%;
  line-height: 113rpx;
}

.home .mpBox .btnBox .active {
  color: #4e88ff !important;
}

.home .mpBox .btnBox view {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background: white;
  text-align: center;
  border: 1rpx solid #f6f6f6;
}

.home .mpBox .btnBox .cancle {
  font-size: 34rpx;
  color: #666666;
}

.home .mpBox .btnBox .confirm {
  font-size: 34rpx;
  color: #bdbdbd;
}

.home .index-loaded-header {
  width: 100%;
  height: 444rpx;
}

.home .index-loaded-header .welcome-block {
  width: 100%;
  height: 299rpx;
  background: url(https://static.58.com/lbg/shangjiaxcxht/zhushou/img/welcome-block.png) no-repeat;
  background-size: 100% auto;
  padding: 50rpx 40rpx 0 40rpx;
  position: relative;
}

.home .index-loaded-header .welcome-block .welcome-title {
  font-size: 36rpx;
  color: #ffffff;
  letter-spacing: 0;
}

.home .index-loaded-header .welcome-block .msg-box {
  position: absolute;
  right: 40rpx;
  top: 50rpx;
  width: 41rpx;
  height: 31rpx;
  background: url(data:image/png;base64,);
  background-size: 100% auto;
}

.home .index-loaded-header .welcome-block .msg-box.new-msg::after {
  width: 20rpx;
  height: 20rpx;
  background: red;
  border-radius: 50%;
  content: '';
  position: absolute;
  right: -9rpx;
  top: -8rpx;
}

.home .index-loaded-header .item-detail {
  width: 698rpx;
  height: 300rpx;
  background: #fff;
  border-radius: 16rpx;
  margin: 0 26rpx;
  position: relative;
  top: -144rpx;
  padding: 30rpx;
}

.home .index-loaded-header .item-detail .haslogo {
  position: absolute;
  width: 83rpx;
  height: 83rpx;
  border-radius: 100rpx;
  top: 50rpx;
  left: 20rpx;
}

.home .index-loaded-header .item-detail .logoPadding {
  padding-left: 90rpx;
}

.home .index-loaded-header .item-detail .item-name {
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 0;
  line-height: 65rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 560rpx;
}

.home .index-loaded-header .item-detail .item-title,
.home .index-loaded-header .item-detail .item-switch {
  font-size: 28rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 60rpx;
}

.home .index-loaded-header .item-detail .item-switch {
  color: #4e88ff;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}

.home .index-loaded-header .item-detail-progress {
  width: 646rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: #f6f6f6;
  border-radius: 16rpx;
  margin-top: 24rpx;
  position: relative;
}

.home .index-loaded-header .item-detail-progress::after {
  display: block;
  position: absolute;
  right: 20rpx;
  top: 37rpx;
  content: '';
  width: 15rpx;
  height: 26rpx;
  background: url("https://static.58.com/lbg/shangjiaxcxht/zhushou/img/arrow-right.png") no-repeat center;
  background-size: 100%;
}

.home .index-loaded-header .item-detail-progress .current-progress {
  border: 1rpx solid rgba(78, 136, 255, 0.8);
  border-radius: 4rpx;
  width: 108rpx;
  height: 38rpx;
  color: #4e88ff;
  font-size: 24rpx;
  text-align: center;
  line-height: 38rpx;
  float: left;
  margin: 30rpx 16rpx 0 18rpx;
}

.home .index-loaded-header .remind-title {
  color: #9b9b9b;
  border: 0;
  font-size: 28rpx;
}

.home .index-loaded-body {
  width: 698rpx;
  height: 430rpx;
  padding: 60rpx 30rpx;
  margin: 30rpx 26rpx;
  background: #fff;
  border-radius: 16rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}

.home .index-loaded-body .index-loaded-item {
  width: 33%;
  margin-bottom: 70rpx;
  text-align: center;
}

.home .index-loaded-body .index-loaded-item .item-img {
  width: 72rpx;
  height: 72rpx;
}

.home .index-loaded-body .index-loaded-item .item-text {
  font-size: 26rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: center;
  line-height: 32rpx;
  margin-top: 6rpx;
}
