@import "./../components/UploadFile.wxss";
view{box-sizing:border-box}.container{font-size:28rpx;margin-bottom:122rpx}.section{margin-top:20rpx;background-color:#FFFFFF;overflow:hidden}.section .location-detail{margin:0 30rpx 30rpx;height:40rpx}.section .location-map{width:100%;height:360rpx;margin-bottom:30rpx}.section .service-detail{height:200rpx;background:#f6f6f6;border-radius:4rpx;margin:0 20rpx 30rpx;padding:20rpx;width:670rpx}.section .item{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between;-webkit-box-align:center;-webkit-align-items:center;align-items:center;padding:23rpx 0;margin:0 30rpx;position:relative;height:89rpx}.section .item .price-font{color:#FF937B}.section .item.border-bottom{border-bottom:1rpx solid #f3f3f3}.section .item.required::before{display:block;content:'*';width:14rpx;height:37rpx;font-size:26rpx;color:#FFA100;position:absolute;left:-15rpx}.section .item input{width:396rpx;height:30rpx;line-height:30rpx;text-align:right;position:relative}.section .item .input-price{margin-right:84rpx}.section .item .input-duration{margin-right:69rpx}.section .item .unit{position:absolute;top:0;right:0;line-height:89rpx;font-size:28rpx;color:#666}.section .item text{color:#BDBDBD;font-size:30rpx}.section .item button.locate{width:110rpx;height:50rpx;border:2rpx solid #83abff;border-radius:4rpx;padding:0;margin:0;font-size:28rpx;color:#83abff;line-height:50rpx;background-color:inherit}.section .item button.locate::after{border:none}button.save{height:102rpx;background-color:#4e88ff;color:#fff;position:fixed;bottom:0;left:0;right:0;border-radius:0;z-index:999}button.save.disabled{background-color:#C5C5C5}button.save::after{content:none}.placeholder{font-size:28rpx;color:#BDBDBD}.icon{display:inline-block}.icon.more{width:25rpx;height:25rpx;background:url("data:image/png;base64,") no-repeat;background-size:auto 25rpx;background-position:right}
