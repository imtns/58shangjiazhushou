.orderlist {
  font-family: PingFangSC-Regular;
}

.orderlist .tabs-wrap {
  display: table;
  width: 100%;
  background: #fff;
  position: fixed;
  top: 0;
  z-index: 100;
  border-top: 2rpx solid #f3f3f3;
}

.orderlist .tabs-wrap > .tab-box {
  display: table-cell;
  text-align: center;
  line-height: 88rpx;
  color: #666666;
}

.orderlist .tabs-wrap > .tab-box.selected {
  color: #4e88ff;
}

.orderlist .tabs-wrap > .tab-box.selected::after {
  display: block;
  content: '';
  width: 40%;
  height: 6rpx;
  margin: 0 auto;
  background: #4e88ff;
  border-radius: 42rpx;
}

.orderlist .orders-ul,
.orderlist .empty-wrap {
  margin-top: 92rpx;
  padding: 20rpx 0;
  background: #f3f3f3;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
}

.orderlist .orders-ul .order-ul-li,
.orderlist .empty-wrap .order-ul-li {
  margin-bottom: 20rpx;
  background: #fff;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row {
  padding: 0 30rpx;
  font-size: 28rpx;
  color: #333333;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.header-row,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.header-row {
  line-height: 88rpx;
  display: table;
  box-sizing: border-box;
  width: 100%;
  border-bottom: 2rpx solid #f3f3f3;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.header-row .order-number,
.orderlist .orders-ul .order-ul-li .order-ul-li-row.header-row .order-status,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.header-row .order-number,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.header-row .order-status {
  white-space: nowrap;
  overflow: hidden;
  display: table-cell;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.header-row .order-status,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.header-row .order-status {
  text-align: right;
  color: #ff552e;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.header-row .order-status.active,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.header-row .order-status.active {
  color: #ff552e;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.header-row .order-status.cancel,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.header-row .order-status.cancel {
  color: #bdbdbd;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.header-row .order-status.finish,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.header-row .order-status.finish {
  color: #333;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.logo-row,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.logo-row {
  padding: 24rpx 30rpx 20rpx;
  display: flex;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.logo-row .order-mplogo,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.logo-row .order-mplogo {
  width: 58rpx;
  height: 58rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  background: #f3f3f3;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.logo-row .order-mpname,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.logo-row .order-mpname {
  flex: 1;
  align-self: center;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 0.48rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicename,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicename {
  font-size: 32rpx;
  color: #333333;
  margin-bottom: 10rpx;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicename .order-servicename-tit,
.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicename .order-servicename-txt,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicename .order-servicename-tit,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicename .order-servicename-txt {
  display: inline-block;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicename .order-servicename-tit,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicename .order-servicename-tit {
  margin-right: 30rpx;
  color: #999;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicename .order-servicename-tit,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicename .order-servicename-tit {
  margin-right: 30rpx;
  color: #999;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicetime,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicetime {
  font-size: 32rpx;
  color: #333333;
  margin-bottom: 10rpx;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicetime .order-servicetime-tit,
.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicetime .order-servicetime-txt,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicetime .order-servicetime-tit,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicetime .order-servicetime-txt {
  display: inline-block;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-servicetime .order-servicetime-tit,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-servicetime .order-servicetime-tit {
  margin-right: 30rpx;
  color: #999;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-createtime,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-createtime {
  font-size: 32rpx;
  color: #333333;
  margin-bottom: 24rpx;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-createtime .order-createtime-tit,
.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-createtime .order-createtime-txt,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-createtime .order-createtime-tit,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-createtime .order-createtime-txt {
  display: inline-block;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.order-createtime .order-createtime-tit,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.order-createtime .order-createtime-tit {
  margin-right: 30rpx;
  color: #999;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row {
  width: 100%;
  display: table;
  line-height: 100rpx;
  box-sizing: border-box;
  border-top: 2rpx solid #f3f3f3;
  margin-top: 24rpx;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row .order-total,
.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row .order-fee,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row .order-total,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row .order-fee {
  display: table-cell;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row .order-fee,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row .order-fee {
  text-align: right;
  color: #ff552e;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row .order-fee .order-fee-txt,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row .order-fee .order-fee-txt {
  display: inline-block;
  font-family: PingFangSC-Semibold;
  font-size: 34rpx;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row .order-fee.active,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row .order-fee.active {
  color: #ff552e;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row .order-fee.cancel,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row .order-fee.cancel {
  color: #bdbdbd;
}

.orderlist .orders-ul .order-ul-li .order-ul-li-row.footer-row .order-fee.finish,
.orderlist .empty-wrap .order-ul-li .order-ul-li-row.footer-row .order-fee.finish {
  color: #333;
}

.orderlist .orders-ul .order-list-nomore,
.orderlist .empty-wrap .order-list-nomore {
  margin-bottom: 20rpx;
  font-size: 28rpx;
  color: #999999;
  text-align: center;
}

.orderlist .font-yellow {
  color: #ff552e;
}

.orderlist .img-no-data {
  display: block;
  width: 180rpx;
  height: 180rpx;
  margin: 200rpx auto 20rpx;
}

.orderlist .txt-no-data {
  line-height: 88rpx;
  text-align: center;
  font-size: 30rpx;
  color: #ccc;
}
