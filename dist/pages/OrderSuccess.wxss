.ordersuccess {
  font-family: PingFangSC-Regular;
  height: 100vh;
  background: #f6f6f6;
}

.ordersuccess .ordersuccess-container {
  padding: 80rpx 0;
  background: #fff;
}

.ordersuccess .ordersuccess-icon {
  display: block;
  width: 144rpx;
  height: 144rpx;
  margin: 0 auto 30rpx;
}

.ordersuccess .ordersuccess-title {
  text-align: center;
  font-size: 32rpx;
  color: #333333;
}

.ordersuccess .ordersuccess-home,
.ordersuccess .ordersuccess-other {
  margin: 0 30rpx;
  border-radius: 4rpx;
  line-height: 94rpx;
  font-size: 32rpx;
  text-align: center;
}

.ordersuccess .ordersuccess-other {
  margin-top: 80rpx;
  background: #4e88ff;
  color: #fff;
}

.ordersuccess .ordersuccess-home {
  margin-top: 30rpx;
  background: #fff;
  color: #333;
}
