.container {
  width: 100%;
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
}

.container .feedback-title {
  font-size: 32rpx;
  color: #333333;
  line-height: 75rpx;
}

.container .feedback-li, .container .lo-circle {
  font-size: 28rpx;
  color: #333333;
  line-height: 55rpx;
  position: relative;
  padding-left: 35rpx;
  margin: 15rpx 0;
}

.container .feedback-li::before, .container .lo-circle::before {
  content: '';
  width: 20rpx;
  height: 20rpx;
  background: #FFD283;
  border-radius: 50%;
  content: '';
  position: absolute;
  left: 0;
  top: 18rpx;
}

.container .lo-circle::before {
  background: #95D395;
}

.container .feedback-li-flex {
  display: flex;
}

.container .feedback-li-flex .feedback-left {
  width: 112rpx;
  line-height: 45rpx;
  font-size: 26rpx;
  color: #666666;
  margin-right: 20rpx;
}

.container .feedback-li-flex .feedback-right {
  flex: 1;
  line-height: 45rpx;
  font-size: 26rpx;
  color: #666666;
}

.container .feedback-text {
  font-size: 26rpx;
  color: #666666;
  line-height: 45rpx;
}

.container .feedback-img {
  width: 100%;
  box-sizing: border-box;
}
